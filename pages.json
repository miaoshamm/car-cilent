{
  "easycom": {
    // 注意一定要放在custom里，否则无效，https://ask.dcloud.net.cn/question/131175
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  },
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/orders/orders",
      "style": {
        "navigationBarTitleText": "我的订单",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/account/account",
      "style": {
        "navigationBarTitleText": "个人中心",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/invoice_list/invoice_list",
      "style": {
        "navigationBarTitleText": "查看发票",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/wish/wish",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/evaluate_list/evaluate_list",
      "style": {
        "navigationBarTitleText": "我的评价",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/privacy_policy/privacy_policy",
      "style": {
        "navigationBarTitleText": "隐私政策",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/terms_of_service/terms_of_service",
      "style": {
        "navigationBarTitleText": "服务条款",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/order_detail/order_detail",
      "style": {
        "navigationBarTitleText": "订单详情",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/bill_rule/bill_rule",
      "style": {
        "navigationBarTitleText": "计费规则",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/pick_up/pick_up",
      "style": {
        "navigationBarTitleText": "预约接送服务",
				"navigationStyle": "custom",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/wish/wish_pay/wish_pay",
      "style": {
        "navigationBarTitleText": "车辆维保服务",
        "enablePullDownRefresh": false
      }
    },
    {
    	"path" : "pages/staff/staff",
    	"style" : 
    	{
    		"navigationBarTitleText" : "职工入口",
    		"enablePullDownRefresh" : false
    	}
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#fff"
  },
  "uniIdRouter": {},
  "tabBar": {
    "borderStyle": "white",
    "custom": true,
    "list": [
      {
        "pagePath": "pages/index/index"
      },
      {
        "pagePath": "pages/orders/orders"
      },
      {
        "pagePath": "pages/account/account"
      }
    ]
  }
}